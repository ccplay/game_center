[run]
omit =
    wsgi
    local_settings
	*/migrations/*
	*/tests/*

data_file = reports/.coverage

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:

[html]
directory = reports/coverage/html_report

[xml]
output = reports/coverage/coverage.xml
